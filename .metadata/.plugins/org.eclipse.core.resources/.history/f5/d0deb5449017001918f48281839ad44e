package assignment.driver;

import java.util.Random;

public class Main {
	public static void main(String[] args){
		int[] randomnum = new int[10];
		Random random = new Random();
		for(int i = 0; i<10; i++){
			randomnum[i] = random.nextInt();
		}
		long start = System.nanoTime();
		sorter(randomnum);
	}
	
	private static int sorter(int[] array){
		int currentMax = array[0], arrayMaxPos = 0;
		for(int iterations = 1; iterations<array.length; iterations++){
			if(currentMax <= array[iterations]){
				currentMax = array[iterations];
				ArrayMaxPos = iterations;
			}else if(currentMax > array[iterations]){
				
			}
		}
		return null;
	}
}
