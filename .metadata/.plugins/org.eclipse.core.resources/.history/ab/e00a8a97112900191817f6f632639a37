package problem3;

import java.util.ArrayList;
import java.util.List;

public class problem3 {
	public static List<Integer> makeList(int N){
		ArrayList<Integer> lst = new ArrayList<>( );
		for( int i = 0; i < N; i++ ){
			lst.add( i ); //O(N)
			lst.trimToSize( );
		}
		
		//line below I added because it was missing
		return lst;		
	}
}
